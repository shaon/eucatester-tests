EUCA_KEY_DIR=$(cd $(dirname ${BASH_SOURCE:-$0}); pwd -P)
export EC2_URL=http://a.b.c.d:8773/services/compute
export S3_URL=http://a.b.c.d:8773/services/objectstorage
export EUARE_URL=http://a.b.c.d:8773/services/Euare
export TOKEN_URL=http://a.b.c.d:8773/services/Tokens
export AWS_AUTO_SCALING_URL=http://a.b.c.d:8773/services/AutoScaling
export AWS_CLOUDWATCH_URL=http://a.b.c.d:8773/services/CloudWatch
export AWS_ELB_URL=http://a.b.c.d:8773/services/LoadBalancing
export EUSTORE_URL=http://emis.eucalyptus.com/
export EC2_PRIVATE_KEY=${EUCA_KEY_DIR}/euca2-admin-1d3a5bdf-pk.pem
export EC2_CERT=${EUCA_KEY_DIR}/euca2-admin-1d3a5bdf-cert.pem
export EC2_JVM_ARGS=-Djavax.net.ssl.trustStore=${EUCA_KEY_DIR}/jssecacerts
export EUCALYPTUS_CERT=${EUCA_KEY_DIR}/cloud-cert.pem
export EC2_ACCOUNT_NUMBER='000000000000'
export EC2_ACCESS_KEY='AKIAAAAAAAAAAAAAAAAA'
export EC2_SECRET_KEY='AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA'
export AWS_ACCESS_KEY='AKIAAAAAAAAAAAAAAAAA'
export AWS_SECRET_KEY='AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA'
export AWS_CREDENTIAL_FILE=${EUCA_KEY_DIR}/iamrc
export EC2_USER_ID='000000000000'
alias ec2-bundle-image="ec2-bundle-image --cert ${EC2_CERT} --privatekey ${EC2_PRIVATE_KEY} --user ${EC2_ACCOUNT_NUMBER} --ec2cert ${EUCALYPTUS_CERT}"
alias ec2-upload-bundle="ec2-upload-bundle -a ${EC2_ACCESS_KEY} -s ${EC2_SECRET_KEY} --url ${S3_URL}"